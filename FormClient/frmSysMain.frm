    End If
      
    If gVar.ClientLoginCheckOver And (Not gVar.ShowMainWindow) And gVar.TCPStateConnected Then
        mXtrStatusBar.FindPane(gID.StatusBarPaneUserInfo).Text = gVar.UserFullName 'Ö÷´°Ìå×´Ì¬ÖÐÏÔÊ¾ÓÃ»§È«Ãû
        Me.Show 'ÏÔÊ¾Ö÷´°Ìå
        Call gfSendClientInfo(gVar.UserComputerName, gVar.UserLoginName, gVar.UserFullName, Me.Winsock1.Item(1)) '°ÑÓÃ»§µÇÂ½ÐÅÏ¢·¢ËÍ¸ø·þÎñ¶Ë
        gVar.ShowMainWindow = True 'ÏÔÊ¾Ö÷´°Ìå±êÖ¾¡£Çø±ð¹Ø±Õ³ÌÐòÊ±µÄÖ÷´°Ìå×´Ì¬
        Call msLoadUserAuthority(gVar.UserAutoID) '¼ÓÔØÈ¨ÏÞ
        
        Dim frmUnload As Form  'Ð¶ÔØµÇÂ½´°¿Ú¡£²»ÖªÎªºÎ£¬Ö±½ÓÓÃUnload frmSysLogin²»ÄÜÐ¶ÔØµô£¬Ã»·´Ó¦¡£
        For Each frmUnload In Forms
            If LCase(frmUnload.Name) = LCase("frmSysLogin") Then
                Unload frmUnload
                Exit For
            End If
        Next
    End If
    
    byteCon = byteCon + 1 '×´Ì¬¼ÆÊ±
    If Not gVar.TCPStateConnected And gVar.UpdateRunOver Then byteChk = byteChk + 1  '¼ì²é¼ÆÊ±£¬Ö»ÔÚÎÞÁ¬½ÓÊ±
    
    If byteCon >= conCon Then
        If (Not gVar.UpdateRunOver) And (Not gfAppExist(gVar.EXENameOfUpdate)) Then 'È¨ÇÒÈç´Ë,½öÅÐ¶Ï½ø³ÌÊÇ·ñ´æÔÚÊÇ²»È«ÃæµÄ
            gVar.UpdateRunOver = True   '¸üÐÂ³ÌÐòÒÑÔËÐÐÍê³É±êÖ¾
            Call gsOpenTheWindow("frmSysLogin", , vbNormal) ''ÏÔÊ¾µÇÂ½´°¿Ú
            gVar.ClientLoginShow = True 'ÉèÖÃÈ«¾Ö±äÁ¿--ÒÑ´ò¿ª¹ýµÇÂ½´°¿Ú
        End If
             
        With Me.Winsock1.Item(1)
            If .State = 7 Then      'ÒÑÁ¬½Ó
                Call msSetClientState(vbGreen)  'ÉèÖÃÁ¬½Ó×´Ì¬
            ElseIf .State = 9 Then  'Á¬½ÓÒì³£
                Call msSetClientState(vbRed)    'ÉèÖÃÒì³£×´Ì¬
            Else                    'Î´Á¬½ÓµÈ
                Call msSetClientState(vbYellow) 'ÉèÖÃÎ´Á¬½Ó×´Ì¬
            End If
        End With
        byteCon = 0 'ÇåÁã¾²Ì¬ÀÛ»ý±äÁ¿
    End If
    
    If byteChk > (gVar.TCPWaitTime + 1) Then  'ÒòÎª·þÎñÆ÷¶ËÒ²ÊÇµÈ´ýgVar.TCPWaitTime²Å¶Ï¿ªÁ¬½Ó£¬ÕâÀïÑÓ³ÙÒ»µã
        byteChk = 0 '¾²Ì¬±äÁ¿ÇåÁã
        If Not gVar.TCPStateConnected Then  'Î´Á¬½Ó×´Ì¬
            If gVar.ClientLoginCheckOver Then   'ÒÑÐ£Ñé¹ýÕËºÅÃÜÂë
                Call gsAlarmAndLogEx("Óë·þÎñÆ÷½¨Á¢Á¬½ÓÊ§°Ü£¬ÇëÈ·ÈÏ·þÎñ¶Ë³ÌÐòÒÑÆô¶¯", "Á¬½Ó¾¯Ê¾")
                Call msUnloadMe(True)
            End If
        End If
    End If
    
End Sub

Private Sub Winsock1_Close(Index As Integer)
