Private Sub Winsock1_DataArrival(Index As Integer, ByVal bytesTotal As Long)
    Dim strGet As String
    Dim byteGet() As Byte
    
    With gArr(Index)
        If Not .FileTransmitState Then
            '×Ö·ûÐÅÏ¢´«Êä×´Ì¬¡ý
            
            Me.Winsock1.Item(Index).GetData strGet  '½ÓÊÕ×Ö·û
            
            If InStr(strGet, gVar.PTClientConfirm) > 0 Then 'ÊÕµ½Òª»Ø¸´·þÎñ¶ËÈ·ÈÏÁ¬½ÓµÄÐÅÏ¢
                Call gfSendInfo(gVar.PTClientIsTrue, Me.Winsock1.Item(Index))
                .Connected = True
                
            ElseIf InStr(strGet, gVar.PTConnectIsFull) Then 'ÊÕµ½·þÎñ¶Ë·¢À´µÄÁ¬½ÓÊýÒÑÂú
                Me.Timer1.Item(Index).Enabled = False
                Call msUnloadMe(True)
                MsgBox "¿Í»§¶ËÓë·þÎñ¶ËÁ¬½ÓÊýÊÜÏÞ£¬ÇëÆäËûÓÃ»§ÍË³öºóÔÙÊÔ£¡", vbCritical, "Á¬½ÓÊýÒÑÂú¾¯¸æ"
                Rem End 'Ó²½áÊø³ÌÐò£¬ÒÔ·ÀÍòÒ»£¿
            
            ElseIf InStr(strGet, gVar.PTDBDataSource) Then  'ÊÕµ½·þÎñÆ÷·¢À´µÄÊý¾Ý¿âÁ¬½ÓÐÅÏ¢
                Call gfRestoreDBInfo(strGet) '½âÎö¼ÓÃÜ¹ýµÄÊý¾Ý¿âÁ¬½ÓÐÅÏ¢
                gVar.RestoreDBInfoOver = True 'Êý¾Ý¿âÁ¬½ÓÐÅÏ¢½ÓÊÕÍê³É±êÖ¾
                
            ElseIf InStr(strGet, gVar.PTConnectTimeOut) Then 'Á¬ÐøÁ¬½ÓÊ±¼äÒÑµ½
                Dim blnTimer As Boolean, tmrEn As VB.Timer
                For Each tmrEn In Me.Timer1 'ÖØÆô¿Í»§¶ËºóÀÏÊÇ·¢ÏÖtimer1(1)²»´æÔÚ£¬È¨ÇÒÓÃ´Ë¼ì²â
                    If tmrEn.Index = Index Then
                        blnTimer = True
                        Exit For
                    End If
                Next
                Set tmrEn = Nothing
                If blnTimer Then Me.Timer1.Item(Index).Enabled = False
                Me.Winsock1.Item(Index).Close
                Call msSetClientState(vbYellow) 'ÉèÖÃÎ´Á¬½Ó×´Ì¬
                MsgBox "Óë·þÎñÆ÷Á¬ÐøÁ¬½ÓÊ±¼äÒÑµ½£¬ÇëÖØÐÂµÇÂ½£¡", vbExclamation, "Á¬½ÓÊ±¼äÏÞÖÆÌáÊ¾"
                gVar.ClientReLoad = True
                If blnTimer Then Me.Timer1.Item(Index).Enabled = True
                
            ElseIf InStr(strGet, gVar.PTFileStart) > 0 Then '¿ÉÒÔ·¢ËÍÎÄ¼þ¸ø·þÎñ¶ËÁËµÄ×´Ì¬
                Call gfSendFile(.FilePath, Me.Winsock1.Item(Index)) '·¢ËÍÎÄ¼þ¸ø·þÎñ¶Ë
                Call gsFormEnable(Me, False)    '½ûÖ¹¿Í»§¶ËÔÙ²Ù×÷
                
            ElseIf InStr(strGet, gVar.PTFileExist) > 0 Then '·þÎñ¶Ë·¢À´¿Í»§¶ËÏëÒªµÄÎÄ¼þ´æÔÚ
                Dim strSize As String, lngInstrSize As Long
                
                lngInstrSize = InStr(strGet, gVar.PTFileSize) '»ñÈ¡¿Í»§¶ËÏëÒªµÄ´æÔÚÓÚ·þÎñ¶ËµÄÎÄ¼þµÄ´óÐ¡
                If lngInstrSize > 0 Then
                    strSize = Mid(strGet, lngInstrSize + Len(gVar.PTFileSize))
                    If IsNumeric(strSize) Then
                        .FileSizeTotal = Val(strSize)
                        Call gfSendInfo(gVar.PTFileSend, Me.Winsock1.Item(Index)) 'Í¨Öª·þÎñ¶Ë¿ÉÒÔ·¢ËÍ¹ýÀ´ÁË
                        '³õÊ¼»¯½ø¶ÈÌõ
                        
                        .FileTransmitState = True
                        Call gsFormEnable(Me, False)    '½ûÖ¹¿Í»§¶ËÔÙ²Ù×÷
                    End If
                End If
                
            ElseIf InStr(strGet, gVar.PTFileNoExist) > 0 Then   '
                MsgBox "ÐèÒªÎÄ¼þ<" & .FileName & ">ÔÚ·þÎñ¶Ë²»´æÔÚ£¡", vbExclamation, "ÎÄ¼þ¾¯¸æ"
                gArr(Index) = gArr(0)
                
            End If
            
            Debug.Print "Client GetInfo:" & strGet, bytesTotal
            '×Ö·ûÐÅÏ¢´«Êä×´Ì¬¡ü
        Else
            'ÎÄ¼þ´«Êä×´Ì¬¡ý
            
            If .FileNumber = 0 Then 'ÉêÇëÎÄ¼þºÅ
                .FileNumber = FreeFile
                Open .FilePath For Binary As #.FileNumber
            End If
            
            ReDim byteGet(bytesTotal - 1)   'ÖØ¶¨ÒåÊý×é´óÐ¡
            Me.Winsock1.Item(Index).GetData byteGet, vbArray + vbByte   '½ÓÊÕÎÄ¼þÐÅÏ¢²¢·ÅÈëÊý×é
            Put #.FileNumber, , byteGet '±£´æ½øÎÄ¼þÖÐ
            .FileSizeCompleted = .FileSizeCompleted + bytesTotal    '¼ÇÂ¼ÒÑ´«Êä´óÐ¡
            '¸üÐÂ½ø¶ÈÌõ
            
            If .FileSizeCompleted >= .FileSizeTotal Then    '´«ÊäÍê³ÉºóµÄÒ»Ð©´¦Àí
                Close #.FileNumber
                Call gsFormEnable(Me, True) '½â³ý¿Í»§¶ËµÄÏÞÖÆ
                gArr(Index) = gArr(0)
                Call gfSendInfo(gVar.PTFileEnd, Me.Winsock1.Item(Index)) '·¢ËÍ½áÊø±êÖ¾
                Debug.Print "Client Received Over"
            End If
            
            'ÎÄ¼þ´«Êä×´Ì¬¡ü
        End If
    End With
    
End Sub

Private Sub Winsock1_Error(Index As Integer, ByVal Number As Integer, Description As String, ByVal Scode As Long, ByVal Source As String, ByVal HelpFile As String, ByVal HelpContext As Long, CancelDisplay As Boolean)
    'Á¬½ÓÒì³£´¦Àí
    
    If Index <> 0 Then
        If gArr(Index).FileTransmitState Then   'Òì³£Ê±Çå¿ÕÎÄ¼þ´«ÊäÐÅÏ¢
            Debug.Print "ClientWinsockError:" & Index & "--" & Err.Number & "  " & Err.Description
            Close
            gArr(Index) = gArr(0)
